{
  "npmScope": "fundamental-ngx",
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": "*",
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "cloud": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "test",
          "lint",
          "e2e"
        ],
        "canTrackAnalytics": false,
        "showUsageWarnings": true
      }
    },
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ]
      }
    }
  },
  "projects": {
    "docs": {
      "tags": [
        "fd"
      ]
    },
    "docs-e2e": {
      "tags": [],
      "implicitDependencies": [
        "docs"
      ]
    },
    "core": {
      "tags": [
        "fd"
      ]
    },
    "platform": {
      "tags": [
        "fdp"
      ]
    }
  }
}
